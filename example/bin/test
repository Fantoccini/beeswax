#!/bin/sh -eux

JOB="au.com.cba.omnia.ebenezer.example.TestJob --hdfs"
PROJECT=example
JAR=${PROJECT}/target/scala-2.10/ebenezer-example*.jar
JAR_NAME=$(basename ${JAR})

./sbt '; project example; clean; abject-jar'
hadoop jar ${JAR} com.twitter.scalding.Tool ${JOB} --env example --db example --table customers
